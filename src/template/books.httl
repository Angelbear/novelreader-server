<html>
<head>
<script>
	function ImgError(source) {
		source.src = "/static/img/placeholder.png";
		source.onerror = "";
		return true;
	}
</script>
</head>
<body>
	<div id="books">
		<!--#if(books)-->
		<table border="1">
			<!--#for(Book book : books)-->
			<tr>
				<td>${book.id}</td>
				<td><a href="/admin/section_list?book_id=${book.id}&page_no=0&json=0"><img
						src="${book.img}" alt="${book.name}" onerror="ImgError(this);"
						style="width: 100px;" /></a></td>
				<td>${book.name}</td>
				<td><a href="${book.url}">${book.url}</a></td>
				<td>${book.from}</td>
				<td>${book.description}</td>
			</tr>
			<!--#end-->
		</table>
		<!--#end-->
	</div>
</body>
</html>